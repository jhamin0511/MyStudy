<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.github.jhamin0511.mystudy">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".application.StudyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyStudy">
        <activity
            android:name=".ui.study.StudyActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.pass.PassedActivity" />
        <activity android:name=".ui.pass.PassedFragActivity" />
        <activity android:name=".ui.bottomnavigation.BottomInActivity" />
    </application>
</manifest>
