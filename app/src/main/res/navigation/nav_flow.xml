<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_flow"
    app:startDestination="@id/navFlowSplashFragment">

    <fragment
        android:id="@+id/navFlowSplashFragment"
        android:name="com.github.jhamin0511.mystudy.ui.navigation.flow.NavFlowSplashFragment"
        tools:layout="@layout/fragment_flow_splash">
        <action
            android:id="@+id/action_navFlowSplashFragment_to_navFlowLoginFragment"
            app:destination="@id/navFlowLoginFragment"
            app:popUpTo="@id/navFlowSplashFragment"
            app:popUpToInclusive="true" />
        <action
            android:id="@+id/action_navFlowSplashFragment_to_navFlowMainFragment"
            app:destination="@id/navFlowMainFragment"
            app:popUpTo="@id/navFlowSplashFragment"
            app:popUpToInclusive="true" />
    </fragment>
    <fragment
        android:id="@+id/navFlowLoginFragment"
        android:name="com.github.jhamin0511.mystudy.ui.navigation.flow.NavFlowLoginFragment"
        android:label="NavFlowLoginFragment"
        tools:layout="@layout/fragment_flow_login">
        <action
            android:id="@+id/action_navFlowLoginFragment_to_navFlowTutorialFragment"
            app:destination="@id/navFlowTutorialFragment" />
        <action
            android:id="@+id/action_navFlowLoginFragment_to_navFlowSignupFragment"
            app:destination="@id/navFlowSignupFragment" />
        <action
            android:id="@+id/action_navFlowLoginFragment_to_navFlowMainFragment"
            app:destination="@id/navFlowMainFragment"
            app:popUpTo="@id/navFlowLoginFragment"
            app:popUpToInclusive="true" />
        <deepLink app:uri="mystudy://login" />
    </fragment>
    <fragment
        android:id="@+id/navFlowTutorialFragment"
        android:name="com.github.jhamin0511.mystudy.ui.navigation.flow.NavFlowTutorialFragment"
        android:label="NavFlowTutorialFragment"
        tools:layout="@layout/fragment_flow_tutorial" />
    <fragment
        android:id="@+id/navFlowSignupFragment"
        android:name="com.github.jhamin0511.mystudy.ui.navigation.flow.NavFlowSignupFragment"
        android:label="NavFlowSignupFragment"
        tools:layout="@layout/fragment_flow_signup">
        <action
            android:id="@+id/action_navFlowSignupFragment_to_navFlowMainFragment"
            app:destination="@id/navFlowMainFragment"
            app:popUpTo="@id/navFlowSignupFragment"
            app:popUpToInclusive="true" />
    </fragment>
    <fragment
        android:id="@+id/navFlowMainFragment"
        android:name="com.github.jhamin0511.mystudy.ui.navigation.flow.NavFlowMainFragment"
        android:label="NavFlowMainFragment"
        tools:layout="@layout/fragment_flow_main">
        <action
            android:id="@+id/action_navFlowMainFragment_to_navFlowLoginFragment"
            app:destination="@id/navFlowLoginFragment"
            app:popUpTo="@id/navFlowMainFragment"
            app:popUpToInclusive="true" />
    </fragment>
    <fragment
        android:id="@+id/navFlowADetailFragment"
        android:name="com.github.jhamin0511.mystudy.ui.navigation.flow.NavFlowADetailFragment"
        android:label="NavFlowADetailFragment"
        tools:layout="@layout/fragment_flow_a_detail">
        <deepLink app:uri="mystudy://detail/a" />
    </fragment>
    <fragment
        android:id="@+id/navFlowBDetailFragment"
        android:name="com.github.jhamin0511.mystudy.ui.navigation.flow.NavFlowBDetailFragment"
        android:label="NavFlowBDetailFragment"
        tools:layout="@layout/fragment_flow_b_detail">
        <deepLink app:uri="mystudy://detail/b" />
    </fragment>
    <fragment
        android:id="@+id/navFlowCDetailFragment"
        android:name="com.github.jhamin0511.mystudy.ui.navigation.flow.NavFlowCDetailFragment"
        android:label="NavFlowCDetailFragment"
        tools:layout="@layout/fragment_flow_c_detail">
        <deepLink app:uri="mystudy://detail/c" />
    </fragment>
    <fragment
        android:id="@+id/navFlowDDetailFragment"
        android:name="com.github.jhamin0511.mystudy.ui.navigation.flow.NavFlowDDetailFragment"
        android:label="NavFlowDDetailFragment"
        tools:layout="@layout/fragment_flow_d_detail">
        <deepLink app:uri="mystudy://detail/d" />
    </fragment>

    <action
        android:id="@+id/action_global_NavFlowADetailFragment"
        app:destination="@id/navFlowADetailFragment" />
    <action
        android:id="@+id/action_global_NavFlowBDetailFragment"
        app:destination="@id/navFlowBDetailFragment" />
    <action
        android:id="@+id/action_global_NavFlowCDetailFragment"
        app:destination="@id/navFlowCDetailFragment" />
    <action
        android:id="@+id/action_global_navFlowDDetailFragment"
        app:destination="@id/navFlowDDetailFragment" />
    <action
        android:id="@+id/action_global_navFlowLoginFragment"
        app:destination="@id/navFlowLoginFragment"
        app:popUpTo="@id/navFlowLoginFragment"
        app:popUpToInclusive="true" />
</navigation>
